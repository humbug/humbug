{
    "name": "humbug/humbug",
    "description": "Humbug is a Mutation Testing framework for PHP. Whereas Code Coverage can tell you what code your tests are executing, Mutation Testing is intended to help you judge how well your unit tests actually perform and where they could be improved.",
    "keywords": ["mutation testing", "testing", "coverage", "unit testing"],
    "homepage": "https://github.com/padraic/humbug",
    "type": "library",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "PÃ¡draic Brady",
            "email": "padraic.brady@gmail.com",
            "homepage": "http://blog.astrumfutura.com"
        }
    ],
    "support": {
        "issues": "https://github.com/padraic/humbug/issues"
    },
    "require": {
        "php": ">=5.4.0",
        "phpunit/phpunit": "~4.4",
        "phpspec/phpspec": "~2.1@dev",
        "symfony/console": "~2.6",
        "symfony/finder": "~2.6",
        "symfony/process": "~2.6",
        "symfony/yaml": "~2.6",
        "sebastian/diff": "~1.1",
        "padraic/phpunit-accelerator": "^1.0.2",
        "padraic/phpunit-extensions": "~1.0@dev",
        "padraic/phpspec-extensions": "~1.0@dev"
    },
    "require-dev": {
        "mockery/mockery": "~0.9"
    },
    "autoload": {
        "psr-4": { "Humbug\\": "src/Humbug/" }
    },
    "autoload-dev": {
        "psr-4": { "Humbug\\Test\\": "tests/Humbug/Test/" }
    },
    "bin": ["bin/humbug"],
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "scripts": {
        "test": "phpunit"
    }
}
